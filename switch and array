// switch opcija //
var dayInWeek=9;
switch(dayInWeek) {
    case 1:
    case 2 :
    case 3:
    case 4:
    case 5:
        console.log("radni dan");
        break;

    case 6:
    case 7:
        console.log("vikend")
        break;
    default:
        console.log("default")

}




//4. number 1-12 ( month in a year//

var monthInYear = 9;
//var timeOfYear = 4
    switch (monthInYear) {
        case 1:
        case 2:
            console.log("february");
            break;
        case 3:
            console.log("winter");
            break;
        case 4:
        case 5:
            console.log("spring");
            break;
        case 6:
            console.log("june");
            break;
        case 7:
        case 8:
            console.log("summer");
            break;

        case 9:
        case 10:
        case 11:
            console.log("fall")
            break;
        case 12:
        default:
            console.log("input should be a number between 1 and 12");
            break;
        
    }


