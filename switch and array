    //-8. aritmeticke operacije using switch//

    var num = 4;
    var num1= 12;
    var st = "+";
    switch(st) {

        case "/":
            console.log(num/num1);
            break;
        case "*":
                console.log(num*num1);
                break;
        case "+":
            console.log(num+num1);
            break;  
    }
